{
  "name": "objection-with-knex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon postgres/index.js",
    "make-seed": "npx knex seed:make dev --knexfile ./postgres/db/knexfile.js",
    "seed": "npx knex seed:run --knexfile ./postgres/db/knexfile.js",
    "migrate" : "npx knex migrate:latest --knexfile ./postgres/db/knexfile.js",
    "rollback" : "npx knex migrate:rollback --knexfile ./postgres/db/knexfile.js",
    "down" : "npx knex migrate:down --knexfile ./postgres/db/knexfile.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "knex": "^2.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.16",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8"
  }
}
